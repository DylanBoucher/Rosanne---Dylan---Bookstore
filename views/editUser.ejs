<!DOCTYPE html>
<html lang="en">
<%- include('partials/head.ejs', { pageName: "New User Profile" }) %>

<body>
    <div> <%- include('partials/head.ejs') %> </div>
    <form action="/products" method="POST">
        <div>
            <label for="username">Update UserName:</label>
            <input type="text" name="title" id="title" value="<%=oneUser.userName%>">
        </div>
        <div>
            <label for="email">Update Email Address: </label>
            <input id="email" name="email" type="text" value="<%=oneUser.email%>"/>
        </div>
        <div>
            <label for="password">Change Password:</label>
            <input type="text" name="password" id="author" value="<%=oneUser.password%>">
        </div>
        <div>
            <label for="image">Update User Image (url): </label>
            <input id="image" name="image" type="text" value="<%=oneUser.image%>"/>
        </div>
        <div>
            <label for="booksSold">Update Books Sold:</label>
            <input type="number" name="booksSold" id="booksSold" value="<%=oneUser.booksSold%>">
        </div>
        <div>
            <label for="location">Update Location (city, state):</label>
            <input type="text" name="location" id="location" value="<%=oneUser.location%>">
        </div>
        <input type="submit" value="Add User">
    </form>
</body>
</html>




